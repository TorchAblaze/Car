<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
  integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
<link rel="stylesheet" type="text/css" href="~/css/styles.css" />

@using Dealership.Models;

<div class="container">
  <div class="card" id="carListPageOne">
    <h1 class="carHeader">Car List</h1>
    <ul class="listList">
      @foreach (Car car in Model)
      {
        <li class="makeModel">@car.MakeModel</li>
        <ul class="listList">
          <li>@car.Miles miles</li>
          <li>$@car.Price.00 </li>
          <hr width="50%">
        </ul>
      }

    </ul>
    <a href="/"><button class="btn btn-outline-warning" id="homeButtonOne" type="button">Return to Home.</button></a>
  </div>
</div>